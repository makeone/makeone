  <!--meta data ends-->



  <title>MakeOne</title>

  <meta http-equiv="content-type" content="text/html; charset=utf-8" />
  <meta http-equiv="Cache-Control" content="no-cache">
  <meta http-equiv="Expires" content="-1">

  <!-- common javascript includes -->
  <%= javascript_include_tag 'prototype' %>
  <%= javascript_include_tag 'common' %>

  <!-- javascript for facebooker auth plugin -->
  <script src="http://static.ak.connect.facebook.com/js/api_lib/v0.4/FeatureLoader.js.php" type="text/javascript"></script>


  <!-- adobe flash stuff -->
  <%= javascript_include_tag 'AC_OETags.js' %>
  <%= javascript_include_tag 'history.js' %>

  <!-- stylesheets -->
  <%= stylesheet_link_tag "common" %>

  <!-- any custom javascript -->
  <script language="JavaScript" type="text/JavaScript">
  <!--

  //-->
  </script>

</head>

<body>

  <!-- facebooker auth --> 
  <%= fb_connect_javascript_tag %>
  <%= init_fb_connect "XFBML" %>



  <div id="main" class="main">
    <div id="header" class="basic">
      <div class="site">
        <div class="logo">
          <a href="/home"><img alt="MakeOne" src="/images/MakeOne_logo_tag.png"/></a>
        </div>
      <div class="topsearch">
        <ul class="nav logged_out nosearch">
          <li <%= if @loc=="home" then 'class="selected"' end %>>
            <a href="/home">Home</a>
          </li>
          <li <%= if @loc=="models" then 'class="selected"' end %>>
            <a href="/models">Models</a>
          </li>
          <li <%= if @loc=="faq" then 'class="selected"' end %>>
            <a href="/faq">FAQ</a>
          </li>
          <li <%= if @loc=="source" then 'class="selected"' end %>>
            <a href="/source">Source</a>
          </li>
          <li <%= if @loc=="login" then 'class="selected"' end %>>

            <% if current_user %>
<!--              <%= link_to "Edit Profile", edit_user_path(:current) %> -->
              <a href="/logout">Logout</a>
            <% else %>
              <a href="/login">Login</a>
            <% end %>

          </li>
          <li>
            <!-- spacer -->
          </li>
        </ul>
      </div>
    </div>
  </div>

<% if flash[:notice] -%>
<div id="notice" class="notice" style="font-size: 1.5em"><%= flash[:notice] %></div>
<% end %>


        <!-- End Header -->
